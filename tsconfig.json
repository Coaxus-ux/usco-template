{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"],
    "paths": {
      "@lib/*": ["src/app/lib/*"],
      "@pages/*": ["src/app/pages/*"],
      "@env/*": ["src/environments/*"],
      "@spartan-ng/ui-accordion-helm": ["./src/app/lib/ui/ui-accordion-helm/src/index.ts"],
      "@spartan-ng/ui-alert-helm": ["./src/app/lib/ui/ui-alert-helm/src/index.ts"],
      "@spartan-ng/ui-alertdialog-helm": ["./src/app/lib/ui/ui-alertdialog-helm/src/index.ts"],
      "@spartan-ng/ui-aspectratio-helm": ["./src/app/lib/ui/ui-aspectratio-helm/src/index.ts"],
      "@spartan-ng/ui-avatar-helm": ["./src/app/lib/ui/ui-avatar-helm/src/index.ts"],
      "@spartan-ng/ui-badge-helm": ["./src/app/lib/ui/ui-badge-helm/src/index.ts"],
      "@spartan-ng/ui-button-helm": ["./src/app/lib/ui/ui-button-helm/src/index.ts"],
      "@spartan-ng/ui-card-helm": ["./src/app/lib/ui/ui-card-helm/src/index.ts"],
      "@spartan-ng/ui-command-helm": ["./src/app/lib/ui/ui-command-helm/src/index.ts"],
      "@spartan-ng/ui-dialog-helm": ["./src/app/lib/ui/ui-dialog-helm/src/index.ts"],
      "@spartan-ng/ui-icon-helm": ["./src/app/lib/ui/ui-icon-helm/src/index.ts"],
      "@spartan-ng/ui-input-helm": ["./src/app/lib/ui/ui-input-helm/src/index.ts"],
      "@spartan-ng/ui-label-helm": ["./src/app/lib/ui/ui-label-helm/src/index.ts"],
      "@spartan-ng/ui-menu-helm": ["./src/app/lib/ui/ui-menu-helm/src/index.ts"],
      "@spartan-ng/ui-popover-helm": ["./src/app/lib/ui/ui-popover-helm/src/index.ts"],
      "@spartan-ng/ui-progress-helm": ["./src/app/lib/ui/ui-progress-helm/src/index.ts"],
      "@spartan-ng/ui-radiogroup-helm": ["./src/app/lib/ui/ui-radiogroup-helm/src/index.ts"],
      "@spartan-ng/ui-scrollarea-helm": ["./src/app/lib/ui/ui-scrollarea-helm/src/index.ts"],
      "@spartan-ng/ui-separator-helm": ["./src/app/lib/ui/ui-separator-helm/src/index.ts"],
      "@spartan-ng/ui-sheet-helm": ["./src/app/lib/ui/ui-sheet-helm/src/index.ts"],
      "@spartan-ng/ui-skeleton-helm": ["./src/app/lib/ui/ui-skeleton-helm/src/index.ts"],
      "@spartan-ng/ui-spinner-helm": ["./src/app/lib/ui/ui-spinner-helm/src/index.ts"],
      "@spartan-ng/ui-switch-helm": ["./src/app/lib/ui/ui-switch-helm/src/index.ts"],
      "@spartan-ng/ui-tabs-helm": ["./src/app/lib/ui/ui-tabs-helm/src/index.ts"],
      "@spartan-ng/ui-toggle-helm": ["./src/app/lib/ui/ui-toggle-helm/src/index.ts"],
      "@spartan-ng/ui-typography-helm": ["./src/app/lib/ui/ui-typography-helm/src/index.ts"],
      "@spartan-ng/ui-table-helm": ["./src/app/lib/ui/ui-table-helm/src/index.ts"],
      "@spartan-ng/ui-hovercard-helm": ["./src/app/lib/ui/ui-hovercard-helm/src/index.ts"],
      "@spartan-ng/ui-checkbox-helm": ["./src/app/lib/ui/ui-checkbox-helm/src/index.ts"],
      "@spartan-ng/ui-tooltip-helm": ["./src/app/lib/ui/ui-tooltip-helm/src/index.ts"]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
